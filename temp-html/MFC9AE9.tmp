@charset "utf-8";
/* CSS Document */

html, body {
	margin: 0;
	padding: 0;
	background-image:;
	background-size: cover;
	background-color: #000;
	background-attachment: fixed;
	-webkit-font-smoothing: antialiased;
}

.htmlbodycolor {
	background-color: #297ACC;
	background-image: none;
}

.navdiv {
	background-color: #FCF5E4;
	margin: 0;
	height: 65px;
}

.navdivoff {
	display: none;
}

.bgvid {
	position: fixed; right: 0; bottom: 0;
	min-width: 100%; min-height: 100%;
	width: auto; height: auto; z-index: -100;
}

.greetingbox {
	height: 300px;
}

.removegreetingbox {
	display: none;
}

.wrapper {
	overflow: hidden;
}

.wrapperoff {
	position:;
	display: none;
	margin: 0;
}

.infoone {
	float: left;
	height: 50px;
	width: 20%;
}

.infotwo {
	overflow: hidden;
	height: 50px;
	width: 60%;
}

.infothree {
	float: right;
 	height: 50px;
	width: 20%;
	margin-top: -50px;
}

.infofour {
	float: left;
	height: 100px;
	width: 25%;
}

.infofive {
	overflow: hidden;
	height: 100px;
	width: 50%;
}

.infosix {
	float: right;
 	height: 100px;
	width: 25%;
	margin-top: -100px;
}

#colorone{
	background-color: #191919;
}

#colortwo {
	background-color: #0054FF;
}

.infooff {
	display: none;
}

.featuredvideo {
	background-color: #4481FC;
	height: 700px;
}


.footer {
	height: 200px;
	background-color: #AF7F11;
}

.mapbox {
	overflow: hidden;
}

.controlsoff {
	display: none;

}

.controlson {
	display: inherit;
	position: absolute;
	float: left;
	height: 383px;
	width: 0%;
	background-color: #FFF;
	padding:0;
	background-image: url(mantwo.jpg);
}

#myMap {
	float: left;
	position: absolute;
	left: -2000px;
	height: 383px;
	width: 100%;
	margin-left: 0%;
}

input {
	height: 35px;
	color: #000;
	background-color: #FFF;
	text-align: center;
}

.mymapoff {
	display: none;
}

.mymapon {
	display: inherit;
}

.videostop {
	display: none;
}
